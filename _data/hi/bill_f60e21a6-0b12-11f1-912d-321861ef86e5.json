{"legislative_session": "2026", "identifier": "HB1510", "title": "RELATING TO LICENSE PLATES.", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": ["Certificates of Inspection", "Enforcement", "License Plates", "Obstruction"], "abstracts": [{"note": "description", "abstract": "Conditions the issuance of a certificate of inspection upon a vehicle not being equipped with any cover or shield installed over the license plate.  Amends the penalty for obstruction of a license plate.  Allows state or county parking enforcement officers to issue citations for vehicles with a cover or shield obstructing the license plate.  Repeals the requirement that a cover or shield be intended to alter or interfere with the visibility, legibility, or identification of a license plate by law enforcement or traffic enforcement systems."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "Prefiled.", "date": "2026-01-14", "organization_id": "~{\"classification\": \"lower\"}", "classification": [], "related_entities": []}], "sponsorships": [{"name": "MATAYOSHI", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"MATAYOSHI\"}", "organization_id": null}, {"name": "KILA", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"KILA\"}", "organization_id": null}], "related_bills": [], "versions": [{"note": "HB1510", "links": [{"url": "https://data.capitol.hawaii.gov/sessions/session2026/bills/HB1510_.HTM", "media_type": "text/html"}, {"url": "https://data.capitol.hawaii.gov/sessions/session2026/bills/HB1510_.PDF", "media_type": "application/pdf"}], "date": "", "classification": ""}], "documents": [], "citations": [], "sources": [{"url": "https://capitol.hawaii.gov/session/measure_indiv.aspx?billtype=HB&billnumber=1510&year=2026", "note": ""}], "extras": {}, "jurisdiction": {"id": "ocd-jurisdiction/country:us/state:hi/government", "name":