{"legislative_session": "2026", "identifier": "HB1663", "title": "RELATING TO REIMBURSEMENT OF PUBLIC OFFICERS AND EMPLOYEES.", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": ["Interest Owed", "Public Employees", "Reimbursement", "Travel Costs"], "abstracts": [{"note": "description", "abstract": "Requires the State or a county to reimburse an officer or employee for work-related travel costs that the officer or employee loaned to the State or a county within thirty calendar days of submitting an approved Statement of Completed Travel.  Requires the State or a county to pay interest on a loan not reimbursed within thirty days until the loan is fully reimbursed.  Obligates an officer or employee to request a complete refund from the travel vendor for travel not taken.  Entitles an officer or employee to reimbursement from the State or a county when a travel vendor offers no or a partial refund."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "Prefiled.", "date": "2026-01-20", "organization_id": "~{\"classification\": \"lower\"}", "classification": [], "related_entities": []}], "sponsorships": [{"name": "SAYAMA", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"SAYAMA\"}", "organization_id": null}, {"name": "GARRETT", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"GARRETT\"}", "organization_id": null}, {"name": "ILAGAN", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"ILAGAN\"}", "organization_id": null}, {"name": "KILA", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"KILA\"}", "organization_id": null}, {"name": "LEE", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"LEE\"}", "organization_id": null}, {"name": "M.", "classification": "primary