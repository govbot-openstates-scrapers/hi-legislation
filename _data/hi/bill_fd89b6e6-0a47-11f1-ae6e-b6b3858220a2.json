{"legislative_session": "2026", "identifier": "HB1626", "title": "RELATING TO YOUTH PENALTIES.", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": ["Community Service", "Court Costs", "Fees", "Fines", "Juvenile Justice", "Minors", "Penalties", "Prohibited"], "abstracts": [{"note": "description", "abstract": "Prohibits the assessment of any fees, fines, or court costs against a person who was adjudicated for an offense committed during the person's minority, or against the person's parent or guardian, and discharges all related debt obligations assessed before the effective date of the Act.  Encourages the use of community service and other programs that employ aina-based principles.  Repeals certain penalties imposed on parents, guardians, or other persons associated with unaccompanied children in streets and unmarried minors in dance halls."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "Prefiled.", "date": "2026-01-20", "organization_id": "~{\"classification\": \"lower\"}", "classification": [], "related_entities": []}], "sponsorships": [{"name": "TARNAS", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"TARNAS\"}", "organization_id": null}, {"name": "AMATO", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"AMATO\"}", "organization_id": null}, {"name": "BELATTI", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"BELATTI\"}", "organization_id": null}, {"name": "GRANDINETTI", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"GRANDINETTI\"}", "organization_id": null}, {"name": "HUSSEY", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"HUSSEY\"}", "organization_id": null}, {"name": "ILAGAN", "classification": "primary", "entity_type": "person", "prim