{"legislative_session": "2026", "identifier": "HB1519", "title": "RELATING TO CAMPAIGN CONTRIBUTIONS.", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": ["CSC", "Immediate Family Members", "Officers", "Prohibited Contributions", "State or County Contractors", "State or County Grantees"], "abstracts": [{"note": "description", "abstract": "Requires disclosure of the names of compensated officers, and their immediate family members, of state or county contractors and grantees with contracts or grants exceeding $250,000.  Prohibits the compensated officers of state or county contractors, and their immediate family members, from making or soliciting certain campaign contributions for the duration of the contract exceeding $250,000.  Prohibits state or county grantees, including compensated officers and their immediate family members, from making or soliciting certain contributions for the duration of the grant exceeding $250,000.  Requires unlawful contributions to be returned to the contributor or escheat to the Hawaii Election Campaign Fund."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "Prefiled.", "date": "2026-01-14", "organization_id": "~{\"classification\": \"lower\"}", "classification": [], "related_entities": []}], "sponsorships": [{"name": "TARNAS", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"TARNAS\"}", "organization_id": null}, {"name": "AMATO", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"AMATO\"}", "organization_id": null}, {"name": "BELATTI", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"BELATTI\"}", "organization_id": null}, {"name": "GARRETT", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"GARRETT\"}", "organization_id": null}, {"name": "GRANDINETTI", "classification"