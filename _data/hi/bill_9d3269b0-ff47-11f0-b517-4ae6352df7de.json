{"legislative_session": "2026", "identifier": "HB1694", "title": "RELATING TO SUSTAINABLE AVIATION FUEL TAX CREDIT.", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": ["Sustainable Aviation Fuel Tax Credit"], "abstracts": [{"note": "description", "abstract": "Establishes a tax credit for sustainable aviation fuel distribution in Hawaii to reduce greenhouse gas emissions. Provides $1 per gallon, increasing by 2 cents per additional 1 per cent emissions reduction, up to $2 per gallon.  Caps total credits at $20,000,000 annually, with carryover provisions. Requires reporting to ensure transparency and compliance. Applies to taxable years after December 31, 2025, and sunsets on December 31, 2035."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "Prefiled.", "date": "2026-01-20", "organization_id": "~{\"classification\": \"lower\"}", "classification": [], "related_entities": []}], "sponsorships": [{"name": "KILA", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"KILA\"}", "organization_id": null}, {"name": "CHUN", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"CHUN\"}", "organization_id": null}, {"name": "EVSLIN", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"EVSLIN\"}", "organization_id": null}, {"name": "HASHEM", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"HASHEM\"}", "organization_id": null}, {"name": "ILAGAN", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"ILAGAN\"}", "organization_id": null}, {"name": "KAHALOA", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"KAHALOA\"}", "organization_id": null}, {"name": "KEOHOKAPU-LEE LOY", "classification": "primary", "entity_type": "person", "prim