{"legislative_session": "2026", "identifier": "SB2027", "title": "RELATING TO SUSTAINABLE AVIATION FUEL TAX CREDIT.", "from_organization": "~{\"classification\": \"upper\"}", "classification": ["bill"], "subject": ["Sustainable Aviation Fuel Tax Credit"], "abstracts": [{"note": "description", "abstract": "Establishes a tax credit for sustainable aviation fuel distribution in Hawaii to reduce greenhouse gas emissions. Provides $1 per gallon, increasing by 2 cents per additional 1 per cent emissions reduction, up to $2 per gallon.  Caps total credits at $20,000,000 annually, with carryover provisions. Requires reporting to ensure transparency and compliance. Applies to taxable years after December 31, 2025, and sunsets on December 31, 2035."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "Pending Introduction.", "date": "2026-01-14", "organization_id": "~{\"classification\": \"upper\"}", "classification": [], "related_entities": []}], "sponsorships": [{"name": "INOUYE", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"INOUYE\"}", "organization_id": null}, {"name": "FUKUNAGA", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"FUKUNAGA\"}", "organization_id": null}, {"name": "KEOHOKALOLE", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"KEOHOKALOLE\"}", "organization_id": null}, {"name": "MCKELVEY", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"MCKELVEY\"}", "organization_id": null}, {"name": "San Buenaventura", "classification": "cosponsor", "entity_type": "person", "primary": false, "person_id": "~{\"name\": \"San Buenaventura\"}", "organization_id": null}], "related_bills": [], "versions": [{"note": "SB2027", "links": [{"url": "https://data.capitol.hawaii.gov/sessions/session2026/bills/SB2027_.HTM", "media_type": "text/html"}, {"url": "htt