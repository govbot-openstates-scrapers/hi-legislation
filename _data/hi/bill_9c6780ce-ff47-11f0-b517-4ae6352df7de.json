{"legislative_session": "2026", "identifier": "HB1573", "title": "RELATING TO HEALTH.", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": ["AG", "Directory", "E-Liquids", "Electronic Smoking Devices", "Marketing Granted Order", "Penalties"], "abstracts": [{"note": "description", "abstract": "Requires electronic smoking device and e-liquid manufacturers to certify to the Department of Attorney General annually that the manufacturer received a Marketing Granted Order from the federal Food and Drug Administration and that the manufacturer is in compliance with federal regulations and state laws.  Requires the Department to compile and make public a directory of all electronic smoking device and e-liquid manufacturers and products duly certified.  Establishes penalties for manufacturers that fail to comply with the certification requirements and for other parties that sell products that are not in the directory."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "Prefiled.", "date": "2026-01-16", "organization_id": "~{\"classification\": \"lower\"}", "classification": [], "related_entities": []}], "sponsorships": [{"name": "TAKAYAMA", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"TAKAYAMA\"}", "organization_id": null}, {"name": "AMATO", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"AMATO\"}", "organization_id": null}, {"name": "BELATTI", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"BELATTI\"}", "organization_id": null}, {"name": "CHUN", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"CHUN\"}", "organization_id": null}, {"name": "EVSLIN", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"EVSLIN\"}", "organization_id": null}, {"name": "GARRETT", 