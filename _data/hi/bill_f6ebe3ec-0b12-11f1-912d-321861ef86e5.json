{"legislative_session": "2026", "identifier": "HB1654", "title": "RELATING TO PUBLIC EMPLOYEES.", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": ["AG", "Agencies", "Boards", "Commissions", "Confidential Complaint Process", "Departments", "Public Employees", "Reports", "Rules", "State and County"], "abstracts": [{"note": "description", "abstract": "Requires the Attorney General to establish a confidential process for persons to anonymously file complaints against public employees.  Requires the Attorney General to review and forward the complaints to the department, board, commission, or agency deemed appropriate by the Attorney General.  Requires the departments, boards, commissions, and agencies to process the complaint pursuant to their standard complaint\u2011processing procedures and report the disposition to the Attorney General.  Requires the Attorney General to adopt rules and submit annual reports to the Legislature."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "Prefiled.", "date": "2026-01-20", "organization_id": "~{\"classification\": \"lower\"}", "classification": [], "related_entities": []}], "sponsorships": [{"name": "SAYAMA", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"SAYAMA\"}", "organization_id": null}, {"name": "HOLT", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"HOLT\"}", "organization_id": null}, {"name": "ILAGAN", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"ILAGAN\"}", "organization_id": null}, {"name": "KILA", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"KILA\"}", "organization_id": null}, {"name": "KITAGAWA", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"KITAGAWA\"}", "organization_id": null}, {"na