{"legislative_session": "2026", "identifier": "SB2119", "title": "RELATING TO REIMBURSEMENT OF PUBLIC OFFICERS AND EMPLOYEES.", "from_organization": "~{\"classification\": \"upper\"}", "classification": ["bill"], "subject": ["Interest Owed", "Public Employees", "Reimbursement", "Travel Costs"], "abstracts": [{"note": "description", "abstract": "Requires the State or a county to reimburse an officer or employee for work-related travel costs that the officer or employee loaned to the State or a county within thirty calendar days of submitting an approved Statement of Completed Travel.  Requires the State or a county to pay interest on a loan not reimbursed within thirty days until the loan is fully reimbursed.  Obligates an officer or employee to request a complete refund from the travel vendor for travel not taken.  Entitles an officer or employee to reimbursement from the State or a county when a travel vendor offers no or a partial refund."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "Pending Introduction.", "date": "2026-01-14", "organization_id": "~{\"classification\": \"upper\"}", "classification": [], "related_entities": []}], "sponsorships": [{"name": "ELEFANTE", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"ELEFANTE\"}", "organization_id": null}, {"name": "CHANG", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"CHANG\"}", "organization_id": null}, {"name": "HASHIMOTO", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"HASHIMOTO\"}", "organization_id": null}, {"name": "RICHARDS", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"RICHARDS\"}", "organization_id": null}, {"name": "San Buenaventura", "classification": "cosponsor", "entity_type": "person", "primary": false, "person_id": "~{\"name\": \"San Buenaventura\"}", "organizatio