{"legislative_session": "2026", "identifier": "SB195", "title": "RELATING TO PUBLIC TRANSPORTATION.", "from_organization": "~{\"classification\": \"upper\"}", "classification": ["bill"], "subject": ["Counties", "Fare Increases", "HDOT", "Mass Transit Fares", "TransitBased Advertisements"], "abstracts": [{"note": "description", "abstract": "Requires the counties, before implementing any new mass transit fare or increasing any existing fare, to first seek revenues from alternative sources, including transit-based advertisements.  Requires a county that decides, after seeking alternative revenue sources, to implement a new or increased mass transit fare to submit a justification report to the Department of Transportation.  Requires the Department of Transportation to reject or approve the proposed new mass transit fare or fare increase within 30 days of receiving a county's justification report."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "Re-Referred to TRS/EIG, WAM.", "date": "2026-01-21", "organization_id": "~{\"classification\": \"upper\"}", "classification": ["referral-committee"], "related_entities": []}, {"description": "Carried over to 2026 Regular Session.", "date": "2025-12-08", "organization_id": "~{\"classification\": \"legislature\"}", "classification": ["carried-over"], "related_entities": []}, {"description": "Referred to TCA/EIG, WAM.", "date": "2025-01-17", "organization_id": "~{\"classification\": \"upper\"}", "classification": ["referral-committee"], "related_entities": [{"name": "Transportation and Culture and the Arts", "entity_type": "organization", "organization_id": null}, {"name": "Energy and Intergovernmental Affairs", "entity_type": "organization", "organization_id": null}, {"name": "Ways and Means", "entity_type": "organization", "organization_id": null}]}, {"description": "Introduced and passed First Reading.", "date": "2025-01-15", "organization_id": "~{\"classification\": \"upper\"}", "classification": ["i