{"legislative_session": "2026", "identifier": "HB1706", "title": "RELATING TO MENTAL HEALTH.", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": ["DHS", "Medicaid", "Mental Health Professionals", "Mental Health Services", "Prospective Payment System", "Reimbursement", "Rules"], "abstracts": [{"note": "description", "abstract": "Expands the services eligible for Medicaid Prospective Payment System reimbursement to include certain services furnished by a federally qualified health center or rural health clinic and provided by mental health professionals under the clinical supervision of a licensed mental health professional.  Requires the Department of Human Services to adopt rules to allow medical assistance reimbursement payments for services provided to recipients eligible for Medicaid benefits under any plan under Medicaid managed care programs in the State by unlicensed mental health professionals under the clinical supervision of a licensed mental health professional.  Requires the Department of Human Services to seek necessary waivers and any additional approvals from the Centers for Medicare and Medicaid Services."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "Prefiled.", "date": "2026-01-20", "organization_id": "~{\"classification\": \"lower\"}", "classification": [], "related_entities": []}], "sponsorships": [{"name": "KEOHOKAPU-LEE LOY", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"KEOHOKAPU-LEE LOY\"}", "organization_id": null}, {"name": "AMATO", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"AMATO\"}", "organization_id": null}, {"name": "BELATTI", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"BELATTI\"}", "organization_id": null}, {"name": "ILAGAN", "classification": "primary", "entity_type": "person", "primary": true, "per